{
  "name": "ai-safety-agent-ui",
  "version": "1.0.0",
  "description": "UI integration for AI safety agent in UNICC sandbox",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "ingest": "ts-node --compiler-options '{\"module\":\"commonjs\"}' tools/ingest-acceptance.ts && ts-node --compiler-options '{\"module\":\"commonjs\"}' tools/ingest-policy.ts",
    "prepare:data": "npm run ingest"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "dompurify": "^3.3.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0"
  },
  "keywords": [
    "ai-safety",
    "unicc",
    "sandbox",
    "react",
    "accessibility"
  ],
  "author": "AI Safety Team",
  "license": "MIT"
}
